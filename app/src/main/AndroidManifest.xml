            tools:replace="android:launchMode" />
        <activity
            android:name=".tensorflow.loginactivity"
            android:label="@string/app_name"
            android:theme="@style/splashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

